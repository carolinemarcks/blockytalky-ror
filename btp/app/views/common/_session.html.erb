<%- if user_signed_in? %>
  <p>Current User: <%= current_user.name %> <br />
  <%= link_to 'Sign out', destroy_user_session_path, :method => :delete %> &nbsp;&nbsp;&nbsp; <%= link_to 'Home', "/" %> </p>
<%- else %>
  <p>
    <%= link_to 'Register', new_user_registration_path %> |
    <%= link_to 'Sign in', new_user_session_path %>
  </p>
<%- end %>
